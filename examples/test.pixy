component NoParameters
	h1 Parameterless component

component OneParameter(test string)
	h1= test

component ElementID
	h1#element-id Element with an ID

component ElementIDExpression(test string)
	h1#element-id= test

component ElementIDDirectExpression(test string)
	h1#element-id!= test

component ElementClass
	h1.my-class Element with a class

component Layout
	html
		head
			title Title
		body
			main Content

component RenderOtherComponent
	ElementID
	h1 Test
